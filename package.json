{
  "name": "@libero/audit",
  "version": "0.1.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json",
    "migrate": "dotenv -e ../.env -- ts-node src/migrate.ts",
    "start:dev": "dotenv -e ../.env -- nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/main.ts | pino-pretty",
    "update:internal": "yarn upgrade -P @libero"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/libero/audit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "author": "libero-npm@elifesciences.org",
  "license": "MIT",
  "dependencies": {
    "@libero/auth-utils": "^0.1.0",
    "@libero/event-bus": "^0.1.3",
    "@libero/event-types": "^0.1.0",
    "@libero/migrator": "^0.1.0",
    "dotenv-cli": "^2.0.1",
    "express": "^4.17.1",
    "funfix": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.4",
    "node-fetch": "^2.6.0",
    "pg": "^7.12.1",
    "pino": "^5.13.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/node-fetch": "^2.5.0",
    "@types/pino": "^5.8.10",
    "@types/uuid": "^3.4.5",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "pino-pretty": "^3.2.1",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2"
  }
}
